package Clases;

public class BuilderBasico extends BuilderPC{
	private Presupuesto presupuesto;
	
	
	
	
	public void setProcesador() {
		this.presupuesto.setProcesador(super.getComponente("Procesador Basico"));
	}
	
	public void setRam() {
		this.presupuesto.setRam(super.getComponente("RAM 8 GB"));
	}
	
}
